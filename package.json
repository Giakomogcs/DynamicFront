{
  "name": "dynamic-front-root",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "concurrently \"npm run api --workspace=server\" \"npm run dev --workspace=client\" --names \"SERVER,CLIENT\" --prefix-colors \"blue,magenta\"",
    "prisma:studio": "npx prisma studio --schema=./server/prisma/schema.prisma",
    "install:all": "npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}